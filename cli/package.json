{
  "name": "claptime-cli",
  "private": true,
  "bin": {
    "claptime": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "claptime-commons": "file:../commons",
    "cli-ux": "^5.3.2",
    "slugify": "^1.3.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "globby": "^8.0.2"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "claptime",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
